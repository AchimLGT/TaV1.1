local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "TA Hub", HidePremium = false, SaveConfig = false, ConfigFolder = "TA Hub"})

local OtoJJ = Window:MakeTab({
	Name = "Oto JJ",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

OtoJJ:AddButton({
	Name = "5",
	Callback = function()
        local messages = {
            "BİR",
            "İKİ",
            "ÜÇ",
            "DÖRT",
            "BEŞ"
        }
        local interval = 2.20  -- Time interval between messages in seconds
        
        local function SendNextMessage()
            local message = messages[1]
            if message then
                game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(message, "All")
                table.remove(messages, 1)
            end
        end
        
        while #messages > 0 do
            SendNextMessage()
            wait(interval)
        end
  	end    
})

