local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

local Window = Library.CreateLib("Ta Hub V1.1", "Sentinel")

local JJ = Window:NewTab("Jumper Jack")

local JJHub = JJ:NewSection("JJ")

JJ:NewButton("5", "JumperJack", function()
    local messages = {
        "BİR",
        "İKİ",
        "ÜÇ",
        "DÖRT",
        "BEŞ"
    }
    local interval = 2.20  -- Time interval between messages in seconds
    
    local function SendNextMessage()
        local message = messages[1]
        if message then
            game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(message, "All")
            table.remove(messages, 1)
        end
    end
    
    while #messages > 0 do
        SendNextMessage()
        wait(interval)
    end
end)

